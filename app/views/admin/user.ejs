</head>
<body>
	
  <%- include('layout/nav') %>
<a class="btn btn-success float-right" href="admin/post/new" role="button" style="margin-right: 100px;">Add New Post</a>
<table id="example" class="display" style="width:100%">
        <thead>
            <tr>
                <th>ID</th>
                <th>Email</th>
                <th>First Name</th>
                <th>Last Name</th>
                <th>Create Time</th>
                <th>Update Time</th>
            
            </tr>
        </thead>
        <tbody> 
            <% if(data.users){ %>
              <% for(var i=0; i < data.users.length; i++) { %>
                <tr>  
                    <td><%= data.users[i].id %></td>
                    <td><%= data.users[i].email %></td>
                    <td><%= data.users[i].firstname %></td>
                      <td><%= data.users[i].lastname %></td>
                      <td><%= data.users[i].created_at %></td>
                      <td><%= data.users[i].updated_at %></td>
                        
                </tr>

               <% } %>
            <% } %>

        </tbody>  
